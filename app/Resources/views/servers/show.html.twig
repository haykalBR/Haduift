{% extends 'base.html.twig' %}

{% block body %}
    <style>
       .all{
          width: 100%;
          height: 300%;
       }
    </style>

    {% set var= server.http~server.ip%}

    {% if  server.portHttp %}
        {% set var=var~":"~server.portHttp %}
    {% endif %}
    {% if  server.link %}
        {% set var=var~"/"~server.link %}
    {% endif %}
   <iframe src="{{ var }}" width="1000" height="800">
   </iframe>
{% endblock %}
